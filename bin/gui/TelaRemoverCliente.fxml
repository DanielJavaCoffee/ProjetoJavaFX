         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="textoEmail" layoutX="14.0" layoutY="95.0" prefHeight="25.0" prefWidth="430.0" />
      <Button fx:id="buttonExcluir" layoutX="392.0" layoutY="174.0" mnemonicParsing="false" onAction="#onButtonExcluirAction" text="Excluir" />
      <Button fx:id="buttonVoltar" layoutX="14.0" layoutY="174.0" mnemonicParsing="false" onAction="#onButtonVoltar" text="Voltar" />
